<!-- @using login.Models -->
@model User

@{
    ViewData["Title"] = "A Web Page";
}
<form asp-action="Register" asp-controller="Home" method="post">
   <div class="row mb-3">
    <label asp-for="FName" class="col-sm-2 col-form-label">First Name</label>
    <div class="col-sm-10">
      <input type="text" asp-for="FName" class="form-control" id="inputEmail3">
      <span style="color:Red;" asp-validation-for="FName"></span>
    </div>
  </div>
  <div class="row mb-3">
    <label asp-for="LName" class="col-sm-2 col-form-label"></label>
    <div class="col-sm-10">
      <input asp-for="LName" type="text" class="form-control" id="inputPassword3">
      <span style="color:Red;" asp-validation-for="LName"></span>
    </div>
  </div>
    <div class="row mb-3">
    <label asp-for="Email" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-10">
      <input asp-for="Email" type="text" class="form-control" id="inputPassword3">
      <span style="color:Red;" asp-validation-for="Email"></span>
    </div>
  </div>
    <div class="row mb-3">
    <label asp-for="Pass" class="col-sm-2 col-form-label">Password</label>
    <div class="col-sm-10">
      <input asp-for="Pass" type="password" class="form-control" id="inputPassword3">
      <span style="color:Red;" asp-validation-for="Pass"></span>
    </div>
  </div>
    <div class="row mb-3">
    <label asp-for="ConfPasword" class="col-sm-2 col-form-label">Confrim Password</label>
    <div class="col-sm-10">
      <input asp-for="ConfPasword" asp-validation-for="ConfPasword" type="password" class="form-control" id="inputPassword3">
      <span style="color:Red;" asp-validation-for="ConfPasword"></span>
    </div>
  </div>
  <button type="submit" class="btn btn-primary">Register</button>
</form>
<button   class="btn btn-primary" ><a style="color: white" href="/login">LogIn</a></button>
